---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/">
	<channel>
	<title>Leon Radley</title>
	<link>http://www.radley.se</link>
	<language>en</language>
	<webMaster>leon@radley.se (Leon Radley)</webMaster>
	<pubDate>{{ site.time | date_to_xmlschema }}</pubDate>
	<copyright>Copyright 2010-{{ site.time | date: '%Y' }}</copyright>
	<ttl>60</ttl>
	<description>Blog of Leon Radley, professional web developer</description>
	{% for post in site.posts limit:20 %}
		<item>
			<title>{{ post.title }}</title>
			<link>http://www.radley.se{{ post.url }}</link>
			<pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
			<guid>http://www.radley.se{{ post.url }}</guid>
			<description>{{ post.content | xml_escape }}</description>
			{% for tag in post.tags %}
				<category domain="http://www.radley.se/tag/{{ tag }}/">{{ tag }}</category>
			{% endfor %}
		</item>
	{% endfor %}
	</channel>
</rss>